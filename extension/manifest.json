{
  "manifest_version": 3,
  "name": "WebPulse Reminder",
  "version": "0.1",
  "description": "Reminds user to check in on mental wellness.",
  "permissions": ["notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
